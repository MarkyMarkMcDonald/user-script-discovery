{
  "manifest_version": 2,
  "name": "Script Discovery",
  "version": "0.1",
  "background": {
    "scripts": ["jquery-2.1.4.js", "background.js"]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "page_action": {
    "default_icon": "1.png",
    "default_popup": "popup.html"
  }
}
